
print("Semana No.10 Ejercicio 1")
mesEntrada = int(input("Ingrese un numero del 1-12"))
mesSalida = ""
                    
match mesEntrada:
    case 1:
        masSalida = "Enero"
    case 2:
        mesSalida = "Febrero"
    case 3:
        mesSalida = "Marzo"
    case 4:
        mesSalida = "Abril"
    case 4:
        mesSalida = "Mayo"
    case 6:
        mesSalida = "Junio"
    case 7:
        mesSalida = "Julio"
    case 8:
        mesSalida = "Agosto"
    case 9:
        mesSalida = "Septiembre"
    case 10:
        mesSalida = "Octubre"
    case 11:
        mesSalida = "Noviembre"
    case 12:
        mesSalida = "Diciempre"
    case _:
        print("Error: El numero a ingresar debe estar contenido entre el 1-12")

print("MES;", mesSalida)
print(f"MES: {mesSalida}")




print("Semana No.10 Ejercicio 2")

a = int(input("Ingrese un numero mayor que 0"))
b = int(input("Ingrese un segundo mayor que 0"))
c = int(input("Ingrese un tercer mayor que 0"))

if(a <= 0 or b <= 0 or c <= 0):
    print("Error: El numero a ingresar debe ser mayor a 0")

if (a>b):
    if(a>c):
        print("El numero A es mayor a B y a C")
    else:
        if(a == c):
            print("A es mayor a B y A es igual a C")
        else:
            print("A es mayor a B y A es diferente a C")

else:
    if(a == b):
        if(a > c):
            print("A es igual a B, A es mayor que C")
        